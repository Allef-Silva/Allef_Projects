#include<iostream>
using namespace std;
bool palindrome(string word)
{
	int length = word.length();
	int i;
	string reverseWord = "";
	
	for(i = length-1; i >= 0; i--)
	{
		reverseWord = reverseWord + word[i];
	}
		
	return reverseWord == word; 
}


int main(){
	string palavra;
	cin>>palavra;
	if(palindrome(palavra) == true) cout<<"is palindrome";
	else cout<<"not palindrome";
	
	
	
	
}
	
	
	
	

